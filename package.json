{
  "name": "node-ts-seed",
  "version": "1.3.0",
  "description": "Typescript backend seed.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "nodemon --watch './src/**/*.ts' --exec 'ts-node' ./src/index.ts",
    "build": "rm -rf lib && tsc",
    "test": "nyc mocha --exit --opts test/mocha.opts 'test/**/*.test.ts'",
    "test:watch": "mocha --watch --watch-extensions ts --opts test/mocha.opts 'test/**/*.test.ts'",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --fix 'src/**/*.ts' 'test/**/*.ts'",
    "migrate:latest": "knex migrate:latest"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "author": "Phi <somosphi.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/somosphi/node-ts-seed"
  },
  "dependencies": {
    "@hapi/joi": "^17.0.2",
    "@somosphi/logger": "^1.4.2",
    "amqplib": "^0.5.3",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "dotenv": "^8.1.0",
    "elastic-apm-node": "^2.16.2",
    "express": "^4.17.1",
    "helmet": "^3.21.0",
    "injection": "^1.7.7",
    "joi": "^14.3.1",
    "knex": "^0.19.5",
    "moment-timezone": "^0.5.25",
    "mysql2": "^1.7.0",
    "supertest": "^4.0.2",
    "typescript-rabbitmq": "0.0.8"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.1",
    "@types/bunyan-format": "^0.2.3",
    "@types/chai": "^4.2.2",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.1",
    "@types/dotenv": "^6.1.1",
    "@types/elastic-apm-node": "^2.7.0",
    "@types/express": "^4.17.1",
    "@types/hapi__joi": "^16.0.6",
    "@types/helmet": "0.0.43",
    "@types/mocha": "^5.2.7",
    "@types/moment-timezone": "^0.5.12",
    "@types/nock": "^10.0.3",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "prettier": "1.19.1",
    "sinon": "^7.4.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-airbnb-typescript-prettier": "^1.4.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2"
  }
}
